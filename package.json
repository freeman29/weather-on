{
  "name": "@freeman29/weather-on",
  "version": "0.0.0-semantically-released",
  "description": "Node CLI weather app.",
  "main": "lib/weather-on.js",
  "scripts": {
    "commit": "git-cz",
    "build": "webpack --env build",
    "check-coverage": "istanbul check-coverage --root coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "dev": "webpack --progress --colors --watch --env dev",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "istanbul cover -x ./test/*.spec.js _mocha -- -R spec --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "_mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.4.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "1.7.2",
    "eslint-loader": "1.6.1",
    "estraverse": "4.2.0",
    "ghooks": "2.0.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.2.0",
    "semantic-release": "^6.3.2",
    "webpack": "2.2.1",
    "yargs": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freeman29/weather-on"
  },
  "keywords": [
    "cli",
    "weather"
  ],
  "author": "Francis Torda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freeman29/weather-on/issues"
  },
  "homepage": "https://github.com/freeman29/weather-on",
  "directories": {
    "test": "test"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
